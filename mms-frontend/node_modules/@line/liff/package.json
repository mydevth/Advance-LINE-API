{
  "name": "@line/liff",
  "version": "2.21.1",
  "main": "dist/lib/index.js",
  "types": "dist/lib",
  "files": [
    "dist/lib"
  ],
  "homepage": "https://developers.line.biz/en/docs/liff/overview/",
  "license": "SEE LICENSE IN README.md",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "dev-server": "port=${PORT:-8080}; protocol=${PROTOCOL:-http}; host=${HOST:-localhost}; webpack-dev-server --entry=./src/liff --output-filename=liff.js --output-library=liff --output-public-path=\"${protocol}://${host}:${port}/dist/\" --port ${port} --host 0.0.0.0 --disable-host-check",
    "dev-server:https": "key=${CERTIFICATE_KEY_FILE:-./.cert/localhost-key.pem}; cert=${CERTIFICATE_CERT_FILE:-./.cert/localhost.pem}; PROTOCOL=https yarn dev-server --https --key=${key} --cert=${cert}",
    "ds": "yarn dev-server",
    "dev:watch:sdk": "webpack -w",
    "test": "jest --passWithNoTests",
    "typecheck:src": "tsc --noEmit",
    "ts:declaration": "tsc --emitDeclarationOnly",
    "typecheck": "yarn typecheck:src",
    "rm:dist": "rm -rf dist",
    "clean": "yarn rm:dist",
    "build_for_deploy": "webpack && cp -r ./dist/${NODE_ENV}/ ./dist/${NODE_ENV}-copied",
    "build:cdn": "yarn build_for_deploy",
    "prebuild:npm": "yarn rm:dist",
    "build:npm": "webpack --config webpack.config.npm.js",
    "postbuild:npm": "yarn ts:declaration",
    "prebuild:prerelease": "yarn rm:dist",
    "build:prerelease": "webpack --config webpack.config.prerelease.js",
    "postbuild:prerelease": "yarn ts:declaration",
    "deploy": "./scripts/deploy.sh"
  },
  "dependencies": {
    "@liff/analytics": "2.21.1",
    "@liff/close-window": "2.21.1",
    "@liff/consts": "2.21.1",
    "@liff/extensions": "2.21.1",
    "@liff/get-friendship": "2.21.1",
    "@liff/get-language": "2.21.1",
    "@liff/get-line-version": "2.21.1",
    "@liff/get-os": "2.21.1",
    "@liff/get-profile": "2.21.1",
    "@liff/get-version": "2.21.1",
    "@liff/hooks": "2.21.1",
    "@liff/i18n": "2.21.1",
    "@liff/init": "2.21.1",
    "@liff/is-api-available": "2.21.1",
    "@liff/is-in-client": "2.21.1",
    "@liff/is-logged-in": "2.21.1",
    "@liff/is-sub-window": "2.21.1",
    "@liff/liff-types": "2.21.1",
    "@liff/login": "2.21.1",
    "@liff/logout": "2.21.1",
    "@liff/native-bridge": "2.21.1",
    "@liff/open-window": "2.21.1",
    "@liff/permanent-link": "2.21.1",
    "@liff/permission": "2.21.1",
    "@liff/ready": "2.21.1",
    "@liff/scan-code-v2": "2.21.1",
    "@liff/send-messages": "2.21.1",
    "@liff/server-api": "2.21.1",
    "@liff/share-target-picker": "2.21.1",
    "@liff/store": "2.21.1",
    "@liff/sub-window": "2.21.1",
    "@liff/use": "2.21.1",
    "@liff/util": "2.21.1",
    "promise-polyfill": "^8.1.3",
    "tslib": "^2.3.0",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "license-webpack-plugin": "2.3.2",
    "string-replace-loader": "2.3.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-loader": "8.3.0",
    "webpack": "4.42.1",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.11.2",
    "webpack-node-externals": "3.0.0"
  },
  "gitHead": "9af2e0816a5cd5547b99e8cadba3ee274b270a9a"
}

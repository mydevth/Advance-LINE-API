{
  "name": "@liff/extensions",
  "version": "2.21.1",
  "description": "@liff/extensions",
  "license": "SEE LICENSE IN README.md",
  "main": "lib/index.js",
  "module": "lib/es/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "build:extensions": "webpack",
    "postbuild:extensions": "./scripts/copy_dist.sh",
    "build:cdn": "yarn build:extensions && rollup -c",
    "build:npm": "rollup -c rollup.config.js",
    "build:watch": "yarn build:npm -w --watch.include './src/**'",
    "dev-server": "yarn build:watch",
    "dev-server:https": "yarn build:watch",
    "build:prerelease": "rollup -c rollup.config.prerelease.js",
    "clean": "rm -rf ./dist ./lib ./tsconfig.tsbuildinfo",
    "deploy": "./scripts/deploy.sh"
  },
  "dependencies": {
    "@liff/add-to-home-screen": "2.21.1",
    "@liff/check-availability": "2.21.1",
    "@liff/consts": "2.21.1",
    "@liff/get-advertising-id": "2.21.1",
    "@liff/get-line-version": "2.21.1",
    "@liff/get-os": "2.21.1",
    "@liff/logger": "2.21.1",
    "@liff/scan-code": "2.21.1",
    "@liff/store": "2.21.1",
    "@liff/util": "2.21.1"
  },
  "devDependencies": {
    "license-webpack-plugin": "2.3.2",
    "terser-webpack-plugin": "2.3.5",
    "ts-loader": "8.3.0",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11"
  },
  "gitHead": "9af2e0816a5cd5547b99e8cadba3ee274b270a9a"
